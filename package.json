{
  "name": "Edulink",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -k \"npm:dev:api\" \"npm:dev:web\"",
    "dev:web": "vite",
    "dev:api": "json-server --watch db.json --port 4000",
    "build": "vite build && npm run postbuild",
    "preview": "vite preview",
    "postbuild": "cp dist/index.html dist/404.html",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "@types/jspdf": "^1.3.3",
    "@vitejs/plugin-vue": "^6.0.1",
    "concurrently": "^9.2.1",
    "gh-pages": "^6.3.0",
    "json-server": "^1.0.0-beta.3",
    "tailwindcss": "^4.1.12",
    "vite": "^7.1.2"
  }
}
